{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "target": "ES6",
    "esModuleInterop": true,
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", "**/*.ts", "**/*.tsx", "**/*.test.tsx"]
}
